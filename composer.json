{
	"name": "gillespieza/genesass",
	"type": "wordpress-theme",
	"description": "GenesassGenuflex child theme for the Genesis Framework",
	"homepage": "https://github.com/gillespieza/genesass-and-genuflex",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "StudioPress",
			"email": "support@studiopress.com"
		},
		{
			"name": "Amanda Dominy",
			"email": "dominy.amanda@gmail.com"
		}
	],
	"require": {
		"php": "^5.2 || ^7",
		"composer/installers": "^1"
	},
	"require-dev": {
		"php": "^5.6 || ^7",
		"dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
		"phpcompatibility/phpcompatibility-wp": "*",
		"squizlabs/php_codesniffer": "^3.3.2",
		"wp-coding-standards/wpcs": "^2.3"
	},
	"config": {
		"sort-order": true,
		"allow-plugins": {
			"composer/installers": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"scripts": {
		"install-codestandards": "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run",
		"phpcs": "phpcs",
		"phpcbf": "phpcbf"
	},
	"support": {
		"issues": "https://github.com/gillespieza/genesass-and-genuflex/issues",
		"source": "https://github.com/gillespieza/genesass-and-genuflex"
	}
}
